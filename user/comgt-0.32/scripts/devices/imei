opengt
set com 115200n81
set senddelay 0.05
waitquiet 2 0.5
let c=0
system "echo \"No Module\" >/var/imei"
:signal
    let c=c+1
    waitquiet 10 0.5
    send "AT+CGSN^m"
    get 2 "^m" $s
    if % != 0 goto signalerror  
    get 2 "^m" $s
    if % != 0 goto signalerror
    print $s  
    goto signalok
:signalerror
    if c <= 3 goto signal
    else goto error
:error
    exit 1
:signalok
    putenv "SIGNAL="$s
    system "echo $SIGNAL>/var/imei"
