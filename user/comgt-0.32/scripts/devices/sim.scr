opengt
set com 115200n81
    set senddelay 0.05
    waitquiet 2 0.5
    let c=0
:signal
    waitquiet 1 0.1
    send "AT^SCKS?^m"
    get 2 "^m" $s
	print $s
	if % != 0 goto error
    get 2 "^m" $s
	print $s
	if % != 0 goto error
    let $s=$right($s,1)
	if $s = "0" print "Inserted"
	else goto error
    waitquiet 1 0.1
    exit 0
:error
	print "Not Ready"
	exit 1
